{
  "name": "accounts_project",
  "nativescript": {
    "id": "org.nativescript.projectname",
    "tns-android": {
      "version": "6.1.0"
    }
  },
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "android": "tns run android --bundle",
    "ios": "tns run ios --bundle",
    "mock:server": "json-server --id Id --watch mocks/accounts.json",
    "mock:hotel:server": "json-server --id _Id  --watch mocks/data.json -p %PORT%",
    "start:proxy": "ng serve --proxy-config proxy.conf.json",
    "start:https:proxy": "ng serve --proxy-config proxy.https.conf.json",
    "start:proxy:https:mock:server": "concurrently — kill-others \"npm run mock:server\" \"npm run start:proxy\"",
    "start:proxy:mock:server": "concurrently — kill-others \"npm run mock:server\" \"npm run start\""
  },
  "private": true,
